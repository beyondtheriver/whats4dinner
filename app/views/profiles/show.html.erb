<div id="banner">
  <h2><%= link_to "What's 4 Dinner", recipes_path %></h2>
</div>
<div id="profile_wrapper">
  <h2><%= @profile.f_name %>'s Profile</h2>
  <p>Name: <%= @profile.f_name %> <%= @profile.l_name %></p>
     <%= button_to 'Edit Profile', edit_profile_path(current_user), method: :get, :class => "form_button" %>
     <%= button_to "Cancel My Account", "/users/#{current_user.id}/delete", method: :delete, :class => "form_button" %>

  <h2><%= @profile.f_name %>'s Recipes</h2>

</div>
