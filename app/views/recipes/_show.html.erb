<div id="recipe_responses">
   <% @response.body.each do |key| puts key %>
      <div class="response_item">
      <%= link_to "/recipes/individual_recipe/#{key['id']}" do %>
         <%= image_tag(key["image"], :alt => "recipe picture", size: '200x200') %>
      <% end %>
         <%= link_to key['title'], "/recipes/individual_recipe/#{key['id']}" %>
      </div>
      <br>
   <% end %>
</div>
